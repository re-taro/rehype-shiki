  "exports": "./distribution/index.js",
  "types": "./distribution/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/rehype-shiki.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
